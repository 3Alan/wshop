/* iconfont */
@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
}

.icon {
  font-family: "iconfont" !important;
  font-size: inherit;
  font-style: normal;
  font-weight: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-search:before {
  content: "\e900";
}
